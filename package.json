{
  "name": "@selectquotelabs/sqform",
  "version": "1.0.0",
  "description": "SQ Form",
  "main": "dist/main.js",
  "private": false,
  "sideEffects": false,
  "scripts": {
    "build": "webpack",
    "lint": "eslint 'src/**/*.js'",
    "lint.fix": "eslint 'src/**/*.js' --fix",
    "lint.staged": "lint-staged",
    "semver": "npm version",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -c .storybook -o .out",
    "nuke-everything": "rm -rf ./dist && rm -rf ./node_modules && rm -f package-lock.json",
    "release.local": "eval $(egrep -v '^#' .env | xargs) npx semantic-release --no-ci --debug",
    "release.dry-run": "eval $(egrep -v '^#' .env | xargs) npx semantic-release --no-ci --debug --dry-run",
    "commit": "npx git-cz",
    "format": "npx prettier --write 'src/**/*.js'",
    "docs": "cd SQFormDocs && npm start"
  },
  "files": [
    "/dist"
  ],
  "commitlint": {
    "rules": {
      "type-enum": [
        2,
        "always",
        [
          "feat",
          "fix",
          "docs",
          "chore",
          "refactor"
        ]
      ]
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SelectQuoteLabs/SQForm.git"
  },
  "keywords": [],
  "author": "SelectQuoteLabs",
  "license": "MIT",
  "homepage": "https://github.com/SelectQuoteLabs/SQForm#readme",
  "dependencies": {
    "formik": "^2.1.4",
    "moment": "^2.29.1",
    "prop-types": "^15.7.2",
    "react-datepicker": "^2.13.0",
    "react-text-mask": "^5.4.3",
    "react-window": "^1.8.5",
    "scplus-shared-components": "https://npm-public.selectquotelabs.com/scplus-shared-components/4.13.2"
  },
  "peerDependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.5.1",
    "@material-ui/lab": "^4.0.0-alpha.46",
    "@material-ui/pickers": "^4.0.0-alpha.11",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.4",
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^11.0.0",
    "@storybook/addon-actions": "^6.0.26",
    "@storybook/addon-docs": "^6.0.26",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-knobs": "^6.0.26",
    "@storybook/addon-links": "^6.0.26",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.0.26",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.0.26",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-react-docgen": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^5.0.1",
    "eslint": "^6.6.0",
    "eslint-config-react-app": "^5.0.2",
    "eslint-plugin-flowtype": "^4.3.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.2",
    "prettier": "^1.18.2",
    "react-scripts": "^4.0.0",
    "storybook-addon-material-ui": "^0.9.0-alpha.20",
    "style-loader": "^2.0.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^4.2.0"
  },
  "browserslist": "last 2 Chrome versions",
  "bugs": {
    "url": "https://github.com/SelectQuoteLabs/SQForm/issues"
  },
  "directories": {
    "doc": "docs"
  }
}
